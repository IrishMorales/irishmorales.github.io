{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.13"
  },
  "scripts": {
    "jekyll": "bundle exec jekyll serve --livereload",
    "tailwind": "tailwindcss -i ./assets/css/main.css -o ./assets/compiled.css --watch",
    "start": "TAILWIND_MODE=watch NODE_ENV=development npm-run-all --parallel jekyll tailwind"
  }
}
